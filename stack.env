# Environment variables are defined here

# Change to true to enable debug logging
DEBUG=false

# Set your timezone, using this as a reference https://en.wikipedia.org/wiki/List_of_tz_database_time_zones
TZ='Europe/London'

# IP address of camera device
CAM_IP=192.168.1.254

# Wireless Access Point parameters
# Change to false if the WLAN connection is being managed elsewhwere e.g on the Docker host
WLAN_CONNECT=true
WLAN_INTERFACE='wlp2s0'
WLAN_NAME='INNOVV_xx_yyyy'
WLAN_PASSWORD='12345678'
WPA_DRIVER='nl80211'
